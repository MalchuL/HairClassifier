   "source": [
    "# Exponential LR gamma\n",
    "import numpy as np\n",
    "MAX_LR = 2e-4\n",
    "MIN_LR = 1e-5\n",
    "EPOCHES = 20\n",
    "\n",
    "\n",
    "gamma =  np.power(MIN_LR / MAX_LR, 1 / EPOCHES)\n",
    "print('gamma for exp lr:', gamma)"
   ]
